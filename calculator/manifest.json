{
  "manifest_version": 3,
  "name": "Calculator App",
  "version": "1.0",
  "description": "A simple calculator app as a Chrome extension.",
  "action": {
    "default_popup": "calculator.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": ["activeTab"]
}
